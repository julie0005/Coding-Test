import java.util.*;

class ReportMail {
    public int[] solution(String[] id_list, String[] report, int k) {
        int[] answer = new int[id_list.length];
        int[] reported = new int[id_list.length];
        boolean[][] reportMap = new boolean[id_list.length][id_list.length];
        for(boolean temp[] : reportMap) {
            Arrays.fill(temp, false);
        }
        
        for(int i=0; i<report.length; i++) {
            String[] parsed = report[i].split(" ");
            int from=0, to=0;
            for(int j=0; j<id_list.length; j++) {
                if(id_list[j].equals(parsed[0])) {
                    from = j;
                }
                if(id_list[j].equals(parsed[1])) {
                    to = j;
                }
            }
            if(!reportMap[from][to]) {
                reported[to]++;
                reportMap[from][to]=true;
            }
        }
        for(int i=0; i<reported.length; i++) {
            if(reported[i] >= k) {
                for(int j=0; j<id_list.length; j++) {
                    if(reportMap[j][i]) answer[j]++;
                }
            }
        }
        return answer;
    }
}