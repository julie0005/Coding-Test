import java.util.*;

class Spy {
    String[][] closet;
    boolean[] visited;
    int answer=0;
    public void dfs(int curIdx, String curCloth){
        //System.out.println(closet[curIdx][0]+" "+closet[curIdx][1]);
        answer++;
        visited[curIdx]=true;
        for(int i=curIdx+1; i<closet.length; i++){
            if(!visited[i] && !curCloth.equals(closet[i][1])){
                dfs(i, closet[i][1]);
            }
        }
        visited[curIdx]=false;
    }
    public int solution(String[][] clothes) {
        closet=clothes;
        visited=new boolean[clothes.length];
        Arrays.sort(closet, new Comparator<String[]>(){
            @Override
            public int compare(String[] s1, String[] s2){
                return s1[1].compareTo(s2[1]);
            }
        });

        for(int i=0; i<closet.length; i++){
            dfs(i, closet[i][1]);
        }
        return answer;
    }
}